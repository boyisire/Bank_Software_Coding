ROUTEDESC=柜台-信用卡校验
[ROUTE]
#如果请求解包后就发现有错误,不继续交易的,置错误标志为1
10 IF NOT(SEQ(FML(ROUTEFLAG),00000000)) THEN GOTO 450
100 CALL 93 WITH  REVMODE 1
120 IF SEQ(MULTI(FML(retcode),1),0) THEN GOTO 500
200 SET ERRSTEP=2
300 SEND 263 WITH END
400 CALL 93 WITH  REVMODE 1
#柜台解包程序校验数据就发生失败的,置错误标志为1
450 SET ERRSTEP=1
500 SEND 263 WITH END

FMTDESC=根据手机号查询客户号-公共请求
TRANTYPE=4
MAC_CREATE=
MAC_CHECK=
[TDF]
10 DO XMLINIT(root,GBK)

20 SET trancode = GETVALUE(/root/pub/trancode)

30 SET CXFS = 2 
40 SET SJHM = IF(SEQ(LEN(TRIM(GETVALUE(/root/pri/SJHM))),0),GETVALUE(/root/pri/MBPHONE),GETVALUE(/root/pri/SJHM))
41 VAR SJHM1 = IF(SEQ(LEN(TRIM(GETVALUE(/root/pri/SJHM))),0),GETVALUE(/root/pri/MBPHONE),GETVALUE(/root/pri/SJHM))
60 SET PCODE = GETVALUE(/root/pub/trancode)
70 SET VALUE = MOD(HASH(VAR(SJHM1)),1024)

1000 DO XMLFREE()

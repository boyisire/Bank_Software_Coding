FMTDESC=根据账号查询客户号-公共请求
TRANTYPE=4
MAC_CREATE=
MAC_CHECK=
[TDF]
10 DO XMLINIT(root,GBK)

20 SET trancode = GETVALUE(/root/pub/trancode)

30 SET CXFS = 3
40 SET ZHKH = IF(SEQ(LEN(TRIM(GETVALUE(/root/pri/ZHKH))),0),GETVALUE(/root/pri/ACCOUNT),GETVALUE(/root/pri/ZHKH))
41 VAR ZHKH1 = IF(SEQ(LEN(TRIM(GETVALUE(/root/pri/ZHKH))),0),GETVALUE(/root/pri/ACCOUNT),GETVALUE(/root/pri/ZHKH))
60 SET PCODE = GETVALUE(/root/pub/trancode)
70 SET VALUE = MOD(HASH(VAR(ZHKH1)),1024)

1000 DO XMLFREE()
